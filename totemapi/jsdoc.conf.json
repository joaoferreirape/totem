{
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "exclude": [
            "node_modules",
            "public",
            "views"
        ],
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "api",
            "app",
            "bin",
            "config",
            "helpers",
            "models",
            "routes",
            "scripts",
            "tests",
            "utils"
        ],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}